<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to fa_icon("twitter", text: "Sign in with #{provider.to_s.titleize}"), omniauth_authorize_path(resource_name, provider), class: 'button small primary expand' %><br />
  <% end -%>
<% end -%>